    ..setNormalAbilities("Aeth,A06E,A005,A001,Ahrp,Avul,AInv")
    ..setIconGameInterface("ReplaceableTextures\\CommandButtons\\BTNMurgalSlave.blp")
    ..setDefaultActiveAbility("Ahrp")
    ..setStructuresBuilt("h01Z,h00N,h00M,h00J,h01B,h00I,h00G,h020,h008,h01Y,h00F")
    ..setButtonPositionX(1)
    ..setModelFile("units\\creeps\\MurgulSlave\\MurgulSlave")
